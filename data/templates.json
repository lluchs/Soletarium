{"features":"<div id=\"features\"><ul>{{#features}}<li {{#if hidden}}style=\"display: none\"{{/if}} data-feature=\"{{id}}\"><div>{{title}}</div></li>{{/features}}</ul></div>","habzone":"<div id=habzone><img src=\"images/features/habZone_left.png\" alt class=left><div class=inner></div><img src=\"images/features/habZone_right.png\" alt class=right></div>","header":"{{#header}}<header class=logo><div><a href=\"{{link}}\"><img src=\"images/layout/{{logo}}\" alt=\"Soletarium - {{subtitle}}\"></a></div></header>{{/header}}","main":"<div id=\"main\">{{> header}}<section id=\"content\"></section>{{#footer}}<footer><section>{{{s1}}}</section><section>{{{s2}}}</section><section>{{{s3}}}</section></footer>{{/footer}}</div>","meta":"<div id=meta><div class=bg></div>{{> header}}{{#meta}}<section><div><div class=op>{{{Operators}}}</div><div class=discl>{{{Disclaimer}}}</div></div><div class=thanks>{{{Thanksto}}}</div><div class=clear></div></section><div class=margin></class>{{/meta}}</div>","missions":"<div class=missions><div class=nano><ul class=content>{{#missions}}<li class=\"{{status}}\"><span class=\"flag {{flag}}\"></span><i>{{year}}</i>: {{mission}}</li>{{/missions}}</ul></div>{{#summary}}<p><span class=total>{{total}}</span> - <span class=success>{{success}}</span> - <span class=partial>{{partial}}</span> - <span class=fail>{{fail}}</p>{{/summary}}<a class=close href=\"#/{{lang}}/main/feature/missions\">X</a></div>","missionshint":"<div id=missionshint><h1>{{title}}</h1>{{hint}}</div>","planetdetail":"<section class=\"planetdetail\"><div class=\"left\"><div class=\"tabs\"><ul>{{#tabs}}<li class=\"tab\" style=\"{{style}}\"><a href=\"{{location}}\">{{title}}</a></li>{{/tabs}}</ul></div><div class=\"content\"></div></div><div class=\"rbar\"><h1>{{title}}</h1><a class=close href=\"#/{{lang}}/main\">X</a></div></section>","planetdetailcontent":"<div class=text><div class=\"nano\"><div class=\"content\">{{{text}}}</div></div></div><div class=image><img><div class=video></div><div class=caption><div><div class=content><h1></h1><div></div></div><div class=line><div></div></div><nav><ol></ol></nav></div></div></div>","planetdetailmedia":"<div class=\"media\">{{#media}}<div class=\"thumb\"><img src=\"{{thumb}}\" alt=\"{{caption}}\"><div class=\"caption\">{{#if smallcaption}}{{{smallcaption}}}{{else}}{{caption}}{{/if}}</div></div>{{/media}}</div>","planets":"<div id=\"planets\"><div class=\"bg\" id=\"bg-end\"><div></div></div><div class=\"bg\" id=\"bg-sun\"><div></div></div>{{#planets}}<div class=\"planet\" id=\"{{id}}\" data-link=\"#/{{../lang}}/main/planet/{{id}}\">{{#moons}}<div class=\"moon name\" {{#caption}}style=\"left: {{x}}px; top: {{y}}px;\"{{/caption}}>{{name}}</div>{{/moons}}<div class=\"name\" {{#caption}}style=\"left: {{x}}px; top: {{y}}px;\"{{/caption}}>{{name}}</div><img src=\"images/planets/{{id}}.png\" alt=\"{{name}}\"></div>{{/planets}}</div>","solwind":"<div id=solwind></div>","suns":"<section id=suns><div class=\"left\"></div><div class=\"right\"></div></section>","temperature":"<div id=temperature><div><div></div></div></div>","versions":"<div id=versions>{{> header}}{{#versions}}<section><h1>{{title}}</h1><table>{{#version}}<tr><td>v{{num}}:</td><td><ul>{{#each v}}<li>{{this}}</li>{{/each}}</ul></td></tr>{{/version}}</table></section>{{/versions}}</div>","video":"<div class=video><video controls width=\"{{width}}\" height=\"{{height}}\"><source src=\"{{url}}.mp4\" type=\"video/mp4\" /><source src=\"{{url}}.webm\" type=\"video/webm\" /></video>{{#if youtube}}<div class=yt><a href=\"http://youtu.be/{{youtube}}\">YouTube (HD)</a></div>{{/if}}</div>"}
